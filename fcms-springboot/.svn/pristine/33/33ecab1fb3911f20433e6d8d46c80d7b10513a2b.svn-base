public class Balance {
	private double amount;
	private List<BalanceAsset> assets;
	public double getAmount() {
		return amount;
	}
	public void setAmount(double amount) {
		this.amount = amount;
	}
	public List<BalanceAsset> getAssets() {
		return assets;
