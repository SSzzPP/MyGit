package com.yhlt.showcase.blockchain.model.dto.deal;

import com.yhlt.showcase.blockchain.model.dto.script.ScriptSig;

/**
 * 交易中的vin
 * @author stone
 * @date 2018年3月5日 上午10:13:46
 */
public class DealRAWVin {
	String txid;
	long vout;
	ScriptSig scriptSig;
	String coinbase;
	long sequence;
	public String getTxid() {
		return txid;
	}
	public void setTxid(String txid) {
		this.txid = txid;
	}
	public long getVout() {
		return vout;
	}
	public void setVout(long vout) {
		this.vout = vout;
	}
	public ScriptSig getScriptSig() {
		return scriptSig;
	}
	public void setScriptSig(ScriptSig scriptSig) {
		this.scriptSig = scriptSig;
	}
	public String getCoinbase() {
		return coinbase;
	}
	public void setCoinbase(String coinbase) {
		this.coinbase = coinbase;
	}
	public long getSequence() {
		return sequence;
	}
	public void setSequence(long sequence) {
		this.sequence = sequence;
	}
	public DealRAWVin() {
		super();
		scriptSig = new ScriptSig();
	}

}
