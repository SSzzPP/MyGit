package com.yhlt.showcase.base.properties;

import org.springframework.boot.context.properties.ConfigurationProperties;
import org.springframework.context.annotation.Configuration;
import org.springframework.stereotype.Component;

@Component
@Configuration
@ConfigurationProperties(prefix = "env.custom", ignoreUnknownFields = false)
public class EnvProperties {

	private String serverStatic;
	private String serverDynamic;
	private String serverName;
	private String appName;
	private String appUrl;
	private String envType;
	private String easyuiVersion;
	private String easyuiTheme;
	private String fileSavePath;
	private String debug;
	private Boolean smsEnable;
	
	private Boolean requestLogEnabled;
	private Boolean requestLogNoException;
	
	private String sensitiveWordUrl;

	/**
	 * ueditor homeUrl
	 */
	private String homeUrl;
	/**
	 * ueditor urlPrefix
	 */
	private String urlPrefix;
	/**
	 * ueditor imagePathFormat
	 */
	private String imagePathFormat;
	/**
	 * ueditor videoPathFormat
	 */
	private String videoPathFormat;
	
	private String fileNotFound;
	
	private String noFile;
	
	private String noUserImg;

	public String getServerStatic() {
		return serverStatic;
	}

	public void setServerStatic(String serverStatic) {
		this.serverStatic = serverStatic;
	}

	public String getServerDynamic() {
		return serverDynamic;
	}

	public void setServerDynamic(String serverDynamic) {
		this.serverDynamic = serverDynamic;
	}

	public String getServerName() {
		return serverName;
	}

	public void setServerName(String serverName) {
		this.serverName = serverName;
	}

	public String getAppName() {
		return appName;
	}

	public void setAppName(String appName) {
		this.appName = appName;
	}

	public String getAppUrl() {
		return appUrl;
	}

	public void setAppUrl(String appUrl) {
		this.appUrl = appUrl;
	}

	public String getEnvType() {
		return envType;
	}

	public void setEnvType(String envType) {
		this.envType = envType;
	}

	public String getEasyuiVersion() {
		return easyuiVersion;
	}

	public void setEasyuiVersion(String easyuiVersion) {
		this.easyuiVersion = easyuiVersion;
	}

	public String getEasyuiTheme() {
		return easyuiTheme;
	}

	public void setEasyuiTheme(String easyuiTheme) {
		this.easyuiTheme = easyuiTheme;
	}

	public String getFileSavePath() {
		return fileSavePath;
	}

	public void setFileSavePath(String fileSavePath) {
		this.fileSavePath = fileSavePath;
	}

	public String getDebug() {
		return debug;
	}

	public void setDebug(String debug) {
		this.debug = debug;
	}

	public Boolean getSmsEnable() {
		return smsEnable;
	}

	public void setSmsEnable(Boolean smsEnable) {
		this.smsEnable = smsEnable;
	}
	
	public Boolean getRequestLogEnabled() {
		return requestLogEnabled;
	}
	
	public Boolean getRequestLogNoException() {
		return requestLogNoException;
	}
	
	public void setRequestLogEnabled(Boolean requestLogEnabled) {
		this.requestLogEnabled = requestLogEnabled;
	}
	
	public void setRequestLogNoException(Boolean requestLogNoException) {
		this.requestLogNoException = requestLogNoException;
	}

	public String getHomeUrl() {
		return homeUrl;
	}

	public void setHomeUrl(String homeUrl) {
		this.homeUrl = homeUrl;
	}

	public String getUrlPrefix() {
		return urlPrefix;
	}

	public void setUrlPrefix(String urlPrefix) {
		this.urlPrefix = urlPrefix;
	}

	public String getImagePathFormat() {
		return imagePathFormat;
	}

	public void setImagePathFormat(String imagePathFormat) {
		this.imagePathFormat = imagePathFormat;
	}

	public String getVideoPathFormat() {
		return videoPathFormat;
	}

	public void setVideoPathFormat(String videoPathFormat) {
		this.videoPathFormat = videoPathFormat;
	}

	public String getNoFile() {
		return noFile;
	}

	public void setNoFile(String noFile) {
		this.noFile = noFile;
	}

	public String getFileNotFound() {
		return fileNotFound;
	}

	public void setFileNotFound(String fileNotFound) {
		this.fileNotFound = fileNotFound;
	}

	public String getNoUserImg() {
		return noUserImg;
	}

	public void setNoUserImg(String noUserImg) {
		this.noUserImg = noUserImg;
	}

	public String getSensitiveWordUrl() {
		return sensitiveWordUrl;
	}

	public void setSensitiveWordUrl(String sensitiveWordUrl) {
		this.sensitiveWordUrl = sensitiveWordUrl;
	}

}